#include<iostream>

using namespace std;

int main(){
    int num1=10,num2=20;
    cout<<"Sum is: "<<num1+num2<<endl;
    cout<<"Difference is: "<<num1-num2<<endl;
    cout<<"Product is: "<<num1*num2<<endl;
    cout<<"Quotient is: "<<num1/num2<<endl;
    cout<<"Remainder is: "<<num1%num2<<endl;
    return 0;
}